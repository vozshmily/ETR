@{
}

@model List<ETR.Models.Entities.RequestList>

<h1>List</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Requestor Name</th>
            <th scope="col">Date Requested</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var requestItem in Model)
        {
            <tr>
                <td>@requestItem.Id</td>
                <td>@requestItem.RequestorName</td>
                <td>@requestItem.RequestDate</td>
                <td>
                    <a>Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>